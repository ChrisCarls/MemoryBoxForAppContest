<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="chris.com.slider" >
    <uses-permission android:name="android.permission.VIBRATE"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="landscape">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".photoExpander"
            android:theme="@style/Base.Theme.AppCompat.Dialog"></activity>
        <activity android:name=".deerExplain"
        android:theme="@style/Base.Theme.AppCompat.Dialog"></activity>
        <activity android:name=".owlExplain"
            android:theme="@style/Base.Theme.AppCompat.Dialog"></activity>
        <activity android:name=".foxExplain"
            android:theme="@style/Base.Theme.AppCompat.Dialog"></activity>
        <activity android:name=".raccoonExplain"
            android:theme="@style/Base.Theme.AppCompat.Dialog"></activity>
        <activity android:name=".bearExplain"
            android:theme="@style/Base.Theme.AppCompat.Dialog"></activity>
    </application>

</manifest>
